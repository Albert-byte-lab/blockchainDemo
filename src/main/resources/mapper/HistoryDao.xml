<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loongrise.dao.HistoryDao">


    <select id="queryHistoryList"  resultType="com.loongrise.entity.History">
		SELECT
		history_id,
        prev_id,
        next_id,
        hashcode,
        epc,
        tid,
        name,
        address,
        date,
        am_id
		FROM
		history
	</select>

    <select id="queryHistoryListByAmId"  resultType="com.loongrise.entity.History">
		SELECT
		history_id,
        prev_id,
        next_id,
        hashcode,
        epc,
        tid,
        name,
        address,
        date,
        am_id
		FROM
		history
		WHERE
		am_id = #{amId};
	</select>

    <insert id="insertHistory" useGeneratedKeys="true" keyColumn="history_id" keyProperty="historyId" parameterType="com.loongrise.entity.History">
        INSERT INTO history(prev_id,next_id,hashcode,epc,tid,name,address,date,am_id)
        VALUES(#{pre.historyId},#{next.historyId},#{hashCode},#{epc},#{tid},#{name},#{address},#{date},#{amId});
	</insert>



</mapper>