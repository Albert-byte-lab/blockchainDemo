<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loongrise.dao.AviationMaterialCateDao">


    <select id="queryAmcById" parameterType="Long" resultType="com.loongrise.entity.AviationMaterialCate">
		SELECT
        am_category_id,
        am_category_name
		FROM
		am_category
		WHERE
		am_category_id = #{amCategoryId};
	</select>

    <select id="queryAllAmcList" resultType="com.loongrise.entity.AviationMaterialCate">
		SELECT
        am_category_id,
        am_category_name
		FROM
		am_category
	</select>

    <insert id="insertRFID" useGeneratedKeys="true" keyColumn="rfid_id" keyProperty="rfidId" parameterType="com.loongrise.entity.RFID">
		INSERT INTO
		rfid(epc,tid)
		VALUES(#{amCategoryName});
	</insert>


</mapper>