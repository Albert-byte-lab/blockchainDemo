<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loongrise.dao.LocalAuthDao">


    <select id="queryLocalAuth"  resultType="com.loongrise.entity.LocalAuth">
		SELECT
      	*
      	FROM
      	local_auth
      	WHERE
      	username=#{username}
      	AND
      	password=#{password}
	</select>





</mapper>